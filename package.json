{
    "name": "openrecord",
    "version": "0.7.3",
    "description": "Active record like ORM for nodejs",
    "keywords": ["orm", "record", "sql", "sqlite3", "postgres", "pg", "mysql", "database", "activerecord"],
    "author": "Philipp Waldmann <philipp.waldmann@s-team.at>",
    "repository": {
      "type": "git",
      "url": "git://github.com/PhilWaldmann/openrecord.git"
    },
    "dependencies": {
        "async": ">= 0.2.x",
        "glob": ">= 3.2.x",
        "validator": ">= 3.1.x",
        "inflection": ">= 1.3.x",
        "knex": ">= 0.5.x",
        "bluebird": ">= 1.0.x"
    },
    "devDependencies": {
        "mocha": ">= 1.13.0",
        "should": ">= 1.3.0",
        "sqlite3": ">= 2.2.x",
        "pg": ">= 2.11.x",
        "coveralls": ">= 2.6.x",
        "mocha-lcov-reporter": ">= 0.0.x",
        "blanket": ">= 1.1.x",
        "markdox": ">= 0.1.x"
    },
    "main": "./lib/store",
    "directories": {
        "test": "./test"
    },
    "config": {
        "blanket": {
          "pattern": "lib",
          "data-cover-never": "node_modules"
        }
    },
    "scripts": {
        "test": "make test"
    },
    "engines": {
        "node": ">=0.8.7"
    }
}
