{
    "name": "openrecord",
    "version": "1.6.3",
    "description": "Active record like ORM for nodejs",
    "keywords": ["orm", "record", "sql", "sqlite3", "postgres", "pg", "mysql", "database", "activerecord", "promise", "rest", "restify", "nested set", "ldap"],
    "author": "Philipp Waldmann <philipp.waldmann@s-team.at>",
    "repository": {
      "type": "git",
      "url": "git://github.com/PhilWaldmann/openrecord.git"
    },
    "dependencies": {
        "async": ">= 0.2.x",
        "glob": ">= 3.2.x",
        "validator": ">= 3.1.x",
        "inflection": ">= 1.3.x",
        "knex": "~ 0.7.x",
				"bluebird": "~ 2.3.x",
        "moment": ">= 2.7.x"
    },
    "devDependencies": {
        "mocha": ">= 1.13.0",
        "should": ">= 1.3.0",
        "sqlite3": ">= 2.2.x",
        "mysql": ">=2.1.x",
        "pg": ">= 2.11.x",
				"restify": ">= 2.7.x",
        "ldapjs": ">= 0.7.x",
        "coveralls": ">= 2.6.x",
        "mocha-lcov-reporter": ">= 0.0.x",
        "blanket": ">= 1.1.x",
        "docu": "latest"
    },
    "main": "./lib/store",
    "directories": {
        "test": "./test"
    },
    "config": {
        "blanket": {
          "pattern": "lib",
          "data-cover-never": "node_modules"
        }
    },
    "scripts": {
        "test": "make test",
        "postpublish": "make docs && make push-docs"
    },
    "engines": {
        "node": ">=0.8.7"
    }
}
