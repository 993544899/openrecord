{
  "name": "openrecord",
  "version": "2.0.0-alpha2",
  "description": "Active record like ORM for nodejs",
  "license": "MIT",
  "keywords": [
    "orm",
    "record",
    "sql",
    "sqlite3",
    "postgres",
    "pg",
    "mysql",
    "oracle",
    "graphql",
    "database",
    "activerecord",
    "promise",
    "rest",
    "restify",
    "nested set",
    "ldap",
    "activedirectory",
    "active directory",
    "ad"
  ],
  "author": "Philipp Waldmann <philipp.waldmann@s-team.at>",
  "repository": {
    "type": "git",
    "url": "git://github.com/PhilWaldmann/openrecord.git"
  },
  "scripts": {
    "test": "npm run lint && npm run mocha",
    "mocha": "NODE_ENV=test TZ='UTC' mocha",
    "lint": "eslint --ext .js lib test",
    "coverage": "NODE_ENV=test istanbul cover _mocha -- --reporter dot",
    "report-coverage": "NODE_ENV=test istanbul cover _mocha -- --reporter mocha-lcov-reporter && cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "async": "^2.6.0",
    "bluebird": "^3.5.1",
    "date-fns": "^1.29.0",
    "inflection": "~ 1.12.x",
    "knex": "~ 0.14.x",
    "validator": "^9.1.1"
  },
  "devDependencies": {
    "blanket": ">= 1.1.x",
    "coveralls": "^3.0.0",
    "docu": "latest",
    "eslint": "^4.11.0",
    "eslint-config-digitalbits": "^1.0.4",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "fibers": "~ 1.x.x",
    "glob": "^7.1.2",
    "graphql": "^0.11.7",
    "istanbul": "^0.4.5",
    "jsdoc-parse": "^3.0.0",
    "jsdoc-to-markdown": "^3.0.2",
    "ldapjs": "latest",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": ">= 0.0.x",
    "mysql": "latest",
    "pg": "latest",
    "restify": "^6.3.4",
    "restify-clients": "^1.5.2",
    "should": "^13.1.3",
    "sqlite3": "latest",
    "webpack": "^3.8.1",
    "webpack-node-externals": "^1.6.0"
  },
  "main": "./store",
  "directories": {
    "test": "./test"
  },
  "config": {
    "blanket": {
      "pattern": "lib",
      "data-cover-never": "node_modules"
    }
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
